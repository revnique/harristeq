        <div id="content">
            <div class="menubar">
                <div class="sidebar-toggler visible-xs">
                    <i class="ion-navicon"></i>
                </div>
                <div class="page-title">
                    Dashboard
                </div>
                <div class="period-select hidden-xs">
                    <form class="input-daterange">
                        <img src="~/images/ajaxloader.gif" width="25" ng-show="showAjax" style="float:left; margin: 4px 15px 0px 0px;" />
                        <div class="input-group input-group-sm">
                            <span class="input-group-addon">
                                <i class="fa fa-calendar-o"></i>
                            </span>
                            <input name="start" type="text" class="form-control datepicker" placeholder="06/27/2014" ng-model="startDate" />
                        </div>

                        <p class="pull-left">to</p>
                        <div class="input-group input-group-sm">
                            <span class="input-group-addon">
                                <i class="fa fa-calendar-o"></i>
                            </span>
                            <input name="end" type="text" class="form-control datepicker" placeholder="07/27/2014" ng-model="endDate" />
                        </div>
                        <button class="btn btn-primary" ng-click="getGarminData()" style="margin: 0px 0px 0px 15px; padding-top:4px; padding-bottom:4px;">pull</button>
                    </form>
                </div>
            </div>
            <div class="content-wrapper">



                @RenderBody()






            </div>
